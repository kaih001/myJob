        <view class="item" wx:for="{{provinceArr}}" wx:for-item="item" bindtap='gotoCityList' data-itemdata='{{item}}'>
            <text class='text_introduce'>{{item.short_name}}</text>              
            <text class="jump"></text>
        </view>
  </view>  
</view>
