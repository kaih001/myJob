.loading {
  width: 180rpx;
  height: 180rpx;
  border-radius: 50%;
  border: 30rpx solid #BEBEBE;
  border-left: 30rpx solid #FFAA00;
  animation: loading 1s linear infinite;
}

@keyframes loading {
  from {
    transform: rotate(0deg);
  }
  to {
    transform: rotate(360deg);
  }}