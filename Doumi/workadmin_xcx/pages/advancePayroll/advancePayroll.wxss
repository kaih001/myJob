page{
  width: 100%;
  height: 100vh;
  background: #f6f6f6;
}
.section{
  width: 100%;
  background: #f6f6f6;
}
.vip{
  width: 724rpx;
  height: 64rpx;
  border: 2rpx solid #F9DAB8;
  background: #FDF2E5;
  border-radius: 8rpx;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 21rpx 0 10rpx;
  box-sizing: border-box;
}
.vip-left,.vip-right{
  display: flex;
  align-items: center;
  color: #E6953C;
}
.vip-icon{
  display: block;
  width: 56rpx;
  height: 44rpx;
  margin-right: 10rpx;
}
.not-vip-icon{
  display: block;
  width: 40rpx;
  height: 40rpx;
  margin-right: 10rpx;
}
.vip-text{
  font-size: 26rpx;
}
.vip-date-text{
  font-size: 20rpx;
}
.content{
  width: 724rpx;
  /* min-height: 568rpx; */
  border-radius: 8rpx;
  background: #ffffff;
  margin: 18rpx auto;
  padding-bottom: 30rpx;
  box-sizing: border-box;
}
.list{
  width: 100%;
  box-sizing: border-box;
  padding-top: 6rpx;
}
.list-item{
  width: 100%;
  height: 130rpx;
  display: flex;
  align-items: center;
  background: #ffffff;
  box-sizing: border-box;
  position: relative;
}
.list-item::after{
  content: '';
  width: 100%;
  height: 2rpx;
  background: #F7F7F7;
  position: absolute;
  bottom: 0;
  left: 16rpx;
  right: 0;
}
.list-item-last::after{
  left: 0;
}
.checkbox-wrapper{
  width: 72rpx;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  box-sizing: border-box;
  padding-right: 16rpx;
}
.select-icon{
  display: block;
  width: 24rpx;
  height: 24rpx;
}
