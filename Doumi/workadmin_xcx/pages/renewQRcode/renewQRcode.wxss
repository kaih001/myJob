/* pages/renewQRcode/renewQRcode.wxss */
Page{
    background: #F2F2F2;
}
.section{
    width: 630rpx;
    background: #FFFFFF;
    border-radius: 40rpx;
    min-height: 1066rpx;
    margin: 60rpx auto 80rpx;
}
.header{
    font-family: PingFangSC-Medium;
    font-size: 40rpx;
    color: #404040;
    text-align: center;
    line-height: 40rpx;
    padding-top: 120rpx;
}
.main-img{
    width: 446rpx;
    height: 350rpx;
    margin: 60rpx auto 0;
}
.main-img .img{
    display: block;
    width: 446rpx;
    height: 350rpx;
}
.main-content{
    width: 504rpx;
    margin: 60rpx auto 0;
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #808080;
    line-height: 56rpx;
}
.pro-name{
    display: flex;
    align-items: center;
    justify-content: center;
}
.pro-name .pro-label{
    color: #808080;
}
.pro-name .pro-text{
    width: 214rpx;
    height: 56rpx;
    color: #1C1C1C;
    overflow:hidden;
	text-overflow:ellipsis;
	white-space:nowrap
}
.pro-tips{
    text-align: center;
}
.footer{
    display: flex;
    align-items: center;
    justify-content: center;
    margin-top: 120rpx;
}
.footer .btn{
    width: 240rpx;
    height: 88rpx;
    line-height: 88rpx;
    text-align: center;
    border: 2rpx solid #CCCCCC;
    border-radius: 4rpx;
    font-size: 34rpx;
}
.footer .cancel{
    margin-right: 70rpx;
    font-family: PingFangSC-Regular;
    color: #404040;
}
.footer .submit{
    background: #FFAA00;
    font-family: PingFangSC-Semibold;
    color: #FFFFFF;
    border: 0;
}
/* 未加入到项目样式 */
.not-project{
    width: 100%;
    height: 100%;
    background: #FFFFFF;
}
.not-project-img{
    width: 750rpx;
    height: 594rpx;
}
.not-project-img .project-img{
    display: block;
    width: 750rpx;
    height: 594rpx;
}
.not-project-content{
    margin-top: 80rpx;
}
.not-project-content .error-title{
    font-family: PingFangSC-Medium;
    font-size: 32rpx;
    color: #404040;
    text-align: center;
    line-height: 32rpx;
}
.not-project-content .error-tips{
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #808080;
    text-align: center;
    line-height: 44rpx;
    margin-top: 32rpx;
}
.not-project-footer{
    display: flex;
    align-items: center;
    justify-content: center;
}
.not-project-footer .not-project-btn{
    width: 710rpx;
    height: 88rpx;
    line-height: 88rpx;
    background: #FFAA00;
    border-radius: 4rpx;
    text-align: center;
    font-size: 34rpx;
    color: #FFFFFF;
    margin-top: 316rpx;
    margin-bottom: 20rpx;
}