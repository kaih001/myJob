/* pages/withdrawal/withdrawalstatus/withdrawalstatus.wxss */
.withdrawalstatus{
  width: 100%;
  height: 100vh;
  background: #F7F7F7;
}
.head{
  padding: 80rpx 0;
}
.head image{
  display: block;
  width: 136rpx;
  height: 136rpx;
  margin: 0 auto 32rpx;
}
.withdrawal_title{
  display: block;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 36rpx;
  color: #333;
  text-align:center;
}
.withrawal_fail{
  display: block;
  font-size: 28rpx;
  color: #FF6600;
  text-align: center
}
.withrawal_success{
  display: block;
  font-size: 28rpx;
  color: #999;
  text-align: center
}
.withdrawaldetail{
  background: #fff;
  padding-left: 24rpx;
  border: 1px solid #E5E5E5;
}
.withdrawaldetail view{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  height: 96rpx;
}
.withdrawaldetail view:first-child{
  border-bottom: 1px solid #E5E5E5;
}
.lableinfo{
  font-size: 32rpx;
  color: #999;
  margin-left: 32rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.labletitle{
  font-size: 32rpx;
  color: #666;
  white-space: nowrap;
}
.withdrawaldetail_btn{
  width: 670rpx;
  height: 96rpx;
  line-height: 96rpx;
  display: block;
  background: #FFAA00;
  border-radius: 10rpx;
  margin:60rpx auto;
  color: #fff;
  font-size: 32rpx;
  text-align: center
}