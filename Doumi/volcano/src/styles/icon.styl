.dmx-icon
  width 12px
  height 12px
  background-size auto 100%
  background-repeat no-repeat
  background-position center center
.dmx-icon-location
  background-image url('data:image/png;base64,')

.dmx-icon-corp
  background-image url('data:image/svg+xml;base64,')

.dmx-icon-search
  background-image url('data:image/png;base64,')

.dmx-icon-close
  background-image url('data:image/png;base64,')

.dmx-icon-arrow
