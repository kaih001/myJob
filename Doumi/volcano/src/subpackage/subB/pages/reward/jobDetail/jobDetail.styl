.dm-footbar .dm-btn-default::after {
  display: none;
}

.dm-footbar .dm-btn-default ~ .dm-btn-default,
.dm-footbar .dm-btn-primary.dm-active {
  position: relative;
}

.dm-footbar .dm-btn-default ~ .dm-btn-default::before,
.dm-footbar .dm-btn-primary.dm-active::before {
  content: '';
  position: absolute;
  top: 10px;
  left: 0;
  bottom: 10px;
  width: 1px;
  background-color: #e5e5e5;
  transform-origin: 0 0;
