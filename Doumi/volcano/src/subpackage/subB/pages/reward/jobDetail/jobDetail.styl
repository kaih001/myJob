.line {
  position: relative;
  padding-bottom: 20px;
}

.line::after {
  content: '';
  position: absolute;
  right: 0;
  bottom: 0;
  left: 0;
  height: 1px;
  background-color: #e5e5e5;
  transform-origin: 0 100%;
  transform: scale3d(1, 0.5, 1);
}

.dm-pannel-hd {
  padding-top: 0;
  padding-bottom: 0;
}

.dm-pannel-title {
  position: relative;
  color: #404040;
  font-size: 16px;
  font-weight: 700;
  line-height: 24px;
  padding: 0 0 15px;
}

.dm-pannel-title::after {
  content: '';
